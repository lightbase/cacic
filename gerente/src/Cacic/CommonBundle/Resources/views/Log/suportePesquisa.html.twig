
<h5>{{ "Resultado do filtro"|trans }}</h5>

<table class="table table-striped table-bordered">
    <thead>
    <tr>
        <th width="5%" style="text-align: center"></th>
        <th width="20%">{{ "Inicio Sessão"|trans }}</th>
        <th width="10%">{{ "Estação Local"|trans }}</th>
        <th width="10%">{{ "Usuário Local"|trans }}</th>
        <th width="10%">{{ "Usuário Remoto"|trans }}</th>
        <th width="10%">{{ "Sigla S.O Remoto"|trans }}</th>
        <th width="20%">{{ "Conexão "|trans }}</th>
    </tr>
    </thead>

    <tbody>

    {% for suporte in suportes %}

        <tr id="item_{{ suporte[0]['idLog'] }}">
            <td style="text-align: center">{{ loop.index }}</td>
            <td id="item_desc_{{ suporte[0]['idLog'] }}">{{ suporte[0].dtHrInicioConexao|date('d/m/Y H:i:s') }}</td>
            <td>{{ suporte['teIpComputador'] }}</td>
            <td>{{ suporte['nmUsuarioCompleto'] }}</td>
            <td>{{ suporte['nmCompletoUsuarioSrv'] }}</td>
            <td>{{ suporte['teSo'] }}</td>
            <td>{{ suporte[0]['teMotivoConexao'] }}</td>
        </tr>

    {% else %}
        <tr>
            <td style="text-align: center;" colspan="7"><b>{{ "NENHUM REGISTRO ENCONTRADO!"|trans }}</b></td>
        </tr>
    {% endfor %}

    </tbody>
</table>