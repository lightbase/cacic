<div class="row-fluid">
    <div class="span12">
        <div class="box grad_colour_black">
            <h2 class="box_head round_top"><i class="icon-list"></i> {{'Resultado da pesquisa' |trans }}</h2>
            <div class="block box_content round_bottom padding_10">

				<table class="table table-striped table-bordered">
				    <thead>
				    <tr>
				        <th width="5%" style="text-align: center"></th>
				        <th width="20%">{{ "Inicio Sessão"|trans }}</th>
				        <th width="10%">{{ "Estação Local"|trans }}</th>
				        <th width="10%">{{ "Usuário Local"|trans }}</th>
				        <th width="10%">{{ "Usuário Remoto"|trans }}</th>
				        <th width="10%">{{ "Sigla S.O Remoto"|trans }}</th>
				        <th width="20%">{{ "Último Contato "|trans }}</th>
				    </tr>
				    </thead>
				
				    <tbody>
				
				    {% for log in logs %}
				
				        <tr>
				            <td style="text-align: center">{{ loop.index }}</td>
				            <td>{{ log.getDtHrInicioConexao()|date('d/m/Y H:i:s') }}</td>
				            <td>{{ log.getTeNodeAddressCli() }}</td>
				            <td>{{ log.getUsuario().getNmUsuarioCompleto() }}</td>
				            <td>{% if log.getSrcacicSessao() %}{{ log.getSrcacicSessao().getNmCompletoUsuarioSrv() }}{% endif %}</td>
				            <td></td>
				            <td>{{ log.getDtHrUltimoContato()|date('d/m/Y H:i:s') }}</td>
				        </tr>
				
				    {% else %}
				        <tr>
				            <td style="text-align: center;" colspan="7"><b>{{ "NENHUM REGISTRO ENCONTRADO!"|trans }}</b></td>
				        </tr>
				    {% endfor %}
				
				    </tbody>
				</table>
                
            </div> <!-- /block -->
        </div> <!-- /box -->
    </div> <!-- /span -->
</div> <!-- /row -->