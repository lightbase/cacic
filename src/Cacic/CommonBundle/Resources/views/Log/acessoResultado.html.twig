<div class="row-fluid">
    <div class="span12">
        <div class="box grad_colour_black">
            <h2 class="box_head round_top"><i class="icon-list"></i> {{'Resultado da pesquisa' |trans }}</h2>
            <div class="block box_content round_bottom padding_10">

				<table class="table table-striped table-bordered">
				    <thead>
				    <tr>
				        <th width="10%" style="text-align: center"></th>
				        <th width="40%">{{ "Usuário"|trans }}</th>
				        <th width="30%" style="text-align: center">{{ "Data"|trans }}</th>
				        <th width="20%" style="text-align: center">{{ "Endereço IP"|trans }}</th>
				    </tr>
				    </thead>
				
				    <tbody>
				    
				    {% for log in logs %}
				
				        <tr>
				            <td style="text-align: center">{{ loop.index }}</td>
				            <td>{{ log['nmUsuarioCompleto'] }}</td>
                            {%  if local == 'pt_BR' %}
				            <td style="text-align: center">{{ log[0].dtAcao|date('d/m/Y H:i:s') }}</td>
                            {% else %}
                            <td style="text-align: center">{{ log[0].dtAcao|date('m/d/Y H:i:s') }}</td>
                            {% endif %}
				            <td style="text-align: center">{{ log[0].teIpOrigem }}</td>
				        </tr>
				
				    {% else %}
				        <tr>
				            <td style="text-align: center;" colspan="4"><b>{{ "NENHUM REGISTRO ENCONTRADO!"|trans }}</b></td>
				        </tr>
				    {% endfor %}
				
				    </tbody>
				</table>
				
			</div> <!-- /block -->
        </div> <!-- /box -->
    </div> <!-- /span -->
</div> <!-- /row -->