<div class="row-fluid">
    <div class="span12">
        <div class="box grad_colour_black">
            <h2 class="box_head round_top"><i class="icon-list"></i> {{'Resultado da pesquisa' |trans }}</h2>
            <div class="block box_content round_bottom padding_10">

				<table class="table table-striped table-bordered">
				    <thead>
				    <tr>
				        <th width="10%"></th>
				        <th width="30%">{{ 'Nome do Computador' }}</th>
				        <th width="20%" style="text-align: center">{{ "IP"|trans }}</th>
				        <th width="20%" style="text-align: center">{{ "Versão CACIC"|trans }}</th>
				        <th width="20%" style="text-align: center">{{ "Última Coleta"|trans }}</th>
				    </tr>
				    </thead>
				
				    <tbody>
				    
				    {% for computador in computadores %}
				
				        <tr>
				            <td style="text-align: center">{{ loop.index }}</td>
				            <td><a href="{{ path('cacic_computador_detalhar', {idComputador: computador.idComputador}) }}" target="_blank">{{ computador.nmComputador }}</td>
				            <td style="text-align: center">{{ computador.teIpComputador }}</td>
                            <td style="text-align: center">{{ computador.teVersaoCacic }}</td>
				            <td style="text-align: center">{{ computador.dtHrUltAcesso|date('d/m/Y H:i:s') }}</td>
				        </tr>
				
				    {% else %}
				        <tr>
				            <td style="text-align: center;" colspan="5"><b>{{ "NENHUM REGISTRO ENCONTRADO!"|trans }}</b></td>
				        </tr>
				    {% endfor %}
				
				    </tbody>
				</table>
				
			</div> <!-- /block -->
        </div> <!-- /box -->
    </div> <!-- /span -->
</div> <!-- /row -->