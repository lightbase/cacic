<form id="form-patrimonio-etiqueta1" class="form-horizontal" action="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) }}" method="post" {{ form_enctype(form) }}>

        <table class="table table-striped table-bordered">

            <tr>
                <td style="text-align: center"><h4>{{ ' Texto da "Etiqueta 1": '|trans }}</h4></td>
                <td style="vertical-align: top">{{ form_widget(form.teEtiqueta)}}</td>
                <td style="text-align: left"><h6>{{ ' Exemplo "Gerência" '|trans }}</h6></td>
            </tr>
            <tr>
                <td style="text-align: center"><h5>{{ ' Plural de texto da "Etiqueta 1":'|trans }}</h5></td>
                <td style="vertical-align: top">{{ form_widget(form.tePluralEtiqueta) }}</td>
                <td style="text-align: left"><h6>{{ ' Exemplo "Gerências" '|trans }}</h6></td>
            </tr>
            <tr>
                <td style="text-align: center"><h5>{{ 'Texto de ajuda da "Etiqueta 1":'|trans }}</h5></td>
                <td style="vertical-align: top">{{ form_widget(form.teHelpEtiqueta) }}</td>
                <td style="text-align: left"><h6>{{ ' Exemplo "Selecione a Gerência de localização este equipamento" '|trans }}</h6></td>
            </tr>
        </table>

    <br />
    <div class="control-group">
        <div class="controls">
            <button type="reset" class="btn">
                <i class="icon-refresh"></i>
                {{ "Resetar Alterações"|trans }}
            </button>
            <button type="submit" class="btn btn-primary">
                <i class="icon-search"></i>
                {{ 'Salvar Alterações'|trans }}
            </button>
        </div>
    </div>
</form>
