{% extends 'CacicCommonBundle::base.html.twig' %}

{% block body %}

    <div class="row-fluid" xmlns="http://www.w3.org/1999/html">

        <div class="span12">
            <div class="box grad_colour_black">
                <h2 class="box_head round_top"><i class="icon-cog"></i>{{ 'Patrimônio'|trans }}</h2>

                <div class="block box_content round_bottom padding_10">

                    <h3>{{ 'Tela de Coleta de Informações de Patrimônio'|trans }} </h3>

                    <br />
                     <p>{{ 'Para configurar a interface da tela de coleta de informações de patrimônio que será exibida aos usuários,'|trans }}
