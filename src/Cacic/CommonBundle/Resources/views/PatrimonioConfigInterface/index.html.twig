                    <br />
                    <center>
                    <img src="{{ asset('bundles/caciccommon/images/tela_patrimon.png') }} " usemap="#Map"/>
                    <map name="Map">
                        <area shape="rect" coords="655,212,784,231" href="#form-patrimonio-etiqueta9"  data-toggle="tab">
                        <area shape="rect" coords="655,168,784,187" href="#form-patrimonio-etiqueta6"  data-toggle="tab">
