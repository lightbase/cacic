{% extends 'CacicWSBundle::common.xml.twig' %}
{%  block bodyXML %}
    <IdComputador>{{  computador.getIdComputador() }}</IdComputador>
    <WebManagerAddress>{{ rede.getTeServCacic() }}</WebManagerAddress>
    <WebServicesFolderName>{{ ws_folder }}</WebServicesFolderName>
    <cs_compress>{{ cs_compress }}</cs_compress>
    <cs_cipher>{{ cs_cipher }}</cs_cipher>
    <INSTALLCACIC.EXE_HASH>{{ configs['INSTALLCACIC.EXE_HASH'] }}</INSTALLCACIC.EXE_HASH>
    <MainProgramName>{{ configs.MainProgramName }}</MainProgramName>
    <LocalFolderName>{{ configs.LocalFolderName }}</LocalFolderName>
{% if nm_device %}
    <NM_DEVICE>nm_device</NM_DEVICE>
{% endif %}
    <STATUS>{{ status }}</STATUS>
{% endblock %}