                                <span class="label label-info">0</span>
                            </a>
                    {% endif %}
        		    <a href="{{ path('cacic_common_homepage') }}" class="btn btn-shortcut"><img src="{{ asset('bundles/caciccommon/images/v3/cacic_logo_gray.png') }}" alt="Dashboard" />
                        CACIC
                        <span class="label label-info box-sizing-fix">0</span>
                    </a>
                </div><!--/span-->

            </div>
            
            <!-- Breadcrumb -->
        	<div class="row-fluid">
                <div class="span12">
                <ul class="breadcrumb">
                  	<li><a href="{{ path('cacic_common_homepage') }}">CACIC</a></li>
                  	{% block breadcrumb %}
