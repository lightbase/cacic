                <p class="navbar-text navbar-right" style="margin-top: 0px;"> {{ "Logado como"|trans }} <a href="{{ path('cacic_usuario_meus_dados') }}">{{ app.user.nmUsuarioAcesso }}</a></p>

            </div><!--/.nav-collapse -->




        </div>
      </div>
    </div>  
    
    <div class="container-fluid">
      <div class="row-fluid">
      <!-- side nav -->

<div id="side_nav" class="span2 sidebar-nav side_nav">
    <a href="{{ path('cacic_common_homepage') }}">
        <span id="logo" class="logo">&nbsp;</span>
    </a>
          {% block sidenav %}
          {% endblock %}
</div><!--/span-->



          
        <!-- main container -->
        <div class="span10">
        
        	<!-- Shortcut buttons -->
            <div class="row-fluid">

                <div class="span12 shortcut box-sizing-fix" id="main_container">
                    {% if bundleExists('CocarBundle') %}
                            <a href="{{ path('cocar_map') }}" class="btn btn-shortcut"><img src="{{ asset('bundles/cocar/images/cocar-logo-24.png') }}" alt="Dashboard" />
