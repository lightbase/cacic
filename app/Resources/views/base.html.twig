     */
    $(document).ready(function(){
        $('.alert').hide();
        {% if app.session.flashbag.get('error')|length > 0 %} $('#msgErro').show(); {% endif %}
        {% if app.session.flashbag.get('notice')|length > 0 %} $('#msgAviso').show(); {% endif %}
